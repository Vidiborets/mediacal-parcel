ly: 'Helvetica Neue', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen,\n    Ubuntu, Cantarell, 'Open Sans', sans-serif;\n  line-height: 1.5;\n  color: #2a2a2a;\n}\n\nbutton,\nlabel {\n  cursor: pointer;\n}\n\ninput {\n  font: inherit;\n}\n\nimg {\n  display: block;\n  max-width: 100%;\n  height: auto;\n}\nbutton,\nlabel {\n  cursor: pointer;\n}\n\ninput {\n  font: inherit;\n}\n\nimg {\n  display: block;\n  max-width: 100%;\n  height: auto;\n}\n\n*,\n*::before,\n*::after {\n  box-sizing: border-box;\n}\n\nbody {\n  font-family: 'Open Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu,\n    Cantarell, sans-serif;\n  line-height: 1.5;\n  background-color: #fafafa;\n}\n\nimg {\n  display: block;\n  max-width: 100%;\n  height: auto;\n}\n\n.card {\n  width: 288px;\n}\n\n.card-text {\n  margin-top: 0;\n  margin-bottom: 10px;\n}\n\n.search-form {\n  padding: 10px;\n}\n\n.card {\n  margin: 10px;\n}\n.form-control {\n  display: flex;\n  width: 600px;\n  height: 40px;\n  padding: 15px;\n  border-radius: 15px;\n  border: 0;\n  box-shadow: 4px 4px 10px rgba(0, 0, 0, 0.06);\n  outline: none;\n}\n.form-control:focus {\n  border: 1px solid var(--accent-text-color);\n}\n.form-group {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin-top: 3%;\n}\n.container {\n  align-items: center;\n  justify-content: center;\n  width: 1200px;\n  margin-left: auto;\n  margin-right: auto;\n  padding-left: 15px;\n  padding-right: 15px;\n}\n.block {\n  position: fixed;\n  display: flex;\n  color: steelblue;\n  text-align: center;\n  padding-bottom: 40px;\n  align-items: center;\n  bottom: 30px;\n  width: 70%;\n  justify-content: center;\n}\n.show-more {\n  width: 150px;\n  height: 30px;\n  position: absolute;\n  bottom: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 15px;\n  border: 0;\n  box-shadow: 4px 4px 10px rgba(0, 0, 0, 0.06);\n  color: var(--accent-link-color);\n}\n.show-more:hover {\n  border: 1px solid var(--accent-text-color);\n  background-color: var(--accent-text-color);\n  color: var(--background-color-list);\n}\n","sass/_list.scss":".section {\n  padding-top: 94px;\n  padding-bottom: 50px;\n  margin-left: -30px;\n}\n.gallery {\n  display: flex;\n  flex-wrap: wrap;\n  margin-left: -30px;\n  margin-top: -30px;\n  list-style: none;\n}\n.list-item {\n  flex-basis: calc((100% - 3 * 30px) / 3);\n  margin-left: 30px;\n  margin-top: 30px;\n}\n.link-item {\n  box-shadow: 0px 1px 1px rgb(0 0 0 / 12%), 0px 4px 4px rgb(0 0 0 / 6%),\n    1px 4px 6px rgb(0 0 0 / 16%);\n  display: block;\n  //   flex-direction: column;\n  text-decoration: none;\n  border: 1px solid #eeeeee;\n  transition: box-shadow 250ms cubic-bezier(0.4, 0, 0.2, 1);\n  //   height: 100%;\n}\n.link-item:hover .stats {\n  opacity: 1;\n  transform: translateY(-100%);\n}\n.photo-card {\n  display: block;\n  width: 100%;\n  height: 100%;\n}\n.item-card {\n  position: relative;\n  overflow: hidden;\n}\n\n.stats {\n  position: absolute;\n  //   padding: 63px 24px 63px 24px;\n\n  opacity: 0;\n  transition: opacity 250ms cubic-bezier(0.4, 0, 0.2, 1),\n    transform 250ms cubic-bezier(0.4, 0, 0.2, 1);\n  display: flex;\n  align-items: center;\n  justify-content: space-around;\n  width: 100%;\n}\n.stats-item {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  flex-direction: column;\n}\n.material-icons {\n  color: var(--icon-color-team);\n}\n.text-color {\n  color: var(--icon-color-team);\n}\n"},"lineCount":null}},"error":null,"hash":"fc5f9f647be8cc07fbe8d5f5cde4aa8d","cacheData":{"env":{}}}